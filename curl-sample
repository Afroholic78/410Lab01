<script>
/* toggle main nav in small screens */
$('a.menu').click(
  function() {
    $('nav[role="navigation"]').toggle();

    if ($('img.menu-img').attr('src') == '/img/icon-hamburger.png') {
      $('img.menu-img').attr('src', '/img/icon-x.png');
    }
    else {
      $('img.menu-img').attr('src', '/img/icon-hamburger.png');
    }

    return false;
  });
</script>

